{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "lib": ["dom", "es6", "es5"],
        "strict": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "esModuleInterop": true,
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node"
    },

    "include": ["src"]
}
